{
  "require": {
    "php": "^7.4",
    "monolog/monolog": "^2.0",
    "symfony/console": "^5.0",
    "symfony/dependency-injection": "^5.0",
    "symfony/config": "^5.0",
    "symfony/yaml": "^5.0",
    "doctrine/orm": "^2.7",
    "symfony/event-dispatcher": "^5.1",
    "ext-json": "*",
    "knplabs/github-api": "^2.14",
    "php-http/guzzle6-adapter": "^2.0",
    "symfony/process": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "pestphp/pest": "^0.1.5",
    "mockery/mockery": "^1.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Devops\\": "src/",
      "Acme\\": "plugin/AcmeProject/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {"Devops\\": "tests/"}
  },
  "bin": [
    "devops"
  ],
  "scripts": {
    "post-install-cmd": [
      "cp -n config/parameters.yaml.dist config/parameters.yaml || true"
    ]
  }
}
